


/*浮動器*/
#fixed{
	padding:10px;
	top:  0;
	right: 0;
	background-attachment:scroll;
	position: fixed;
	z-index: 999;
	background: #ffffff;
	opacity:1.0;
	box-shadow:2px 2px 2px 2px  rgba(0, 0, 0, 0.3);
}
/*浮動器*/


/*表格*/
#mytable{
	padding:2px;
	margin:auto;
}

td{ 
	padding:15px;
}

/*表格*/


/*搜尋框*/
.input {
	box-sizing: border-box;
	background-repeat: no-repeat;
	width:100px;
	padding:0px;
	border: none;
	border-bottom: 1px solid #ddd;
}
/*搜尋框*/



/*開關按鈕*/
.dropbtn {
	background-color: #4CAF50;
	color: white;
	padding: 5px;
	width: auto;
	border: none;
	cursor: pointer;
}

.dropbtn:hover, .dropbtn:focus {
	background-color: #3e8e41;
}
/*開關按鈕*/



/*標籤List*/
.container {
	margin-top: 20px;
	overflow: hidden;
}
/*標籤List*/



